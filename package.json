{
  "name": "website-11ty-stefanimhoff.de",
  "version": "0.0.1",
  "description": "Personal website build with 11ty",
  "scripts": {
    "build": "npm-run-all build:rollup build:html",
    "build:html": "eleventy",
    "build:rollup": "rollup --config rollup.config.js",
    "clean": "npx del dist",
    "debug": "DEBUG=* npx eleventy",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint src/postcss/**/*.css",
    "lint:css:fix": "npm run lint:css --fix",
    "lint:js": "eslint --ignore-path .gitignore . --ext js",
    "lint:js:fix": "npm run lint:js --fix",
    "plop": "plop",
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "start": "npm run watch",
    "watch": "npm-run-all --parallel watch:*",
    "watch:html": "eleventy --serve",
    "watch:rollup": "rollup --config rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kogakure/website-11ty-stefanimhoff.de.git"
  },
  "keywords": [
    "11ty"
  ],
  "author": "Stefan Imhoff",
  "license": "ISC",
  "homepage": "https://www.stefanimhoff.de",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
    "eleventy-plugin-reading-time": "0.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "markdown-it-external-anchor": "^1.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-github-headings": "^2.0.0",
    "markdown-it-spoiler": "^1.1.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "plop": "^2.7.4",
    "postcss": "^8.2.8",
    "postcss-100vh-fix": "^1.0.2",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.0.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-nested": "^5.0.5",
    "postcss-sort-media-queries": "^3.7.7",
    "prettier": "^2.2.1",
    "rollup": "^2.45.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sal.js": "^0.8.4",
    "stylelint": "^13.12.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-high-performance-animation": "^1.5.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "uglify-js": "^3.13.3"
  }
}
