{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "prettier/prettier": "error"
  }
}
